<!DOCTYPE jdo PUBLIC
    "-//Sun Microsystems, Inc.//DTD Java Data Objects Metadata 2.0//EN"
    "http://java.sun.com/dtd/jdo_2_0.dtd">

<jdo>
    <package name="org.ecocean.external">

	<class name="ExternalBase" identity-type="application">
			<inheritance strategy="complete-table"/>
			<field name="id" primary-key="true">
        			<column length="36"/>
			</field>
			<field name="version">
				<index />
                		<column jdbc-type="BIGINT" allows-null="false"/>
			</field>
	</class>


	<class name="ExternalUser" identity-type="application">
	</class>

	<class name="ExternalSubmission" identity-type="application">
	</class>

	<class name="ExternalOrganization" identity-type="application">
	</class>

    </package>
</jdo>
